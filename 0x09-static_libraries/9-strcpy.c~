#include "main.h"

/**
 * _strcpy - copies the string pointed to by src to the buffer dest
 * @dest: buffer to copy to
 * @src: string to copy
 *
 * Return: pointer to dest
 */
char *_strcpy(char *dest, const char *src)
{
	int i;

	for (i = 0; src[i] != '\0'; i++)
	{
		dest[i] = src[i];
	}
	dest[i] = '\0';

	return (dest);
}
